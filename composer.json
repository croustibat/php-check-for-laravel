{
    "name": "croustibat/composer-check",
    "description": "Interactive CLI tool to check and update outdated Composer dependencies",
    "keywords": [
        "composer",
        "dependencies",
        "outdated",
        "update",
        "ci",
        "security",
        "audit",
        "packages",
        "cli",
        "php"
    ],
    "homepage": "https://github.com/croustibat/composer-check",
    "license": "MIT",
    "authors": [
        {
            "name": "Croustibat",
            "email": "me@croustib.at",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/collections": "^10.0|^11.0|^12.0",
        "laravel/prompts": "^0.1|^0.2|^0.3",
        "symfony/console": "^6.0|^7.0",
        "symfony/process": "^6.0|^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "pestphp/pest": "^3.0",
        "phpstan/phpstan": "^1.12|^2.0",
        "phpstan/extension-installer": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Croustibat\\ComposerCheck\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Croustibat\\ComposerCheck\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/composer-check"
    ],
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint",
        "check": ["@format", "@analyse", "@test"]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Croustibat\\ComposerCheck\\Laravel\\ComposerCheckServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
